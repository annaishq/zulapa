{
  "exclude": [
    "node_modules",
    "output"
  ],
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "es6",
      "es2017",
      "dom"
    ],
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "noImplicitAny": true,
    "outDir": "output",
    "resolveJsonModule": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es6",
    "baseUrl": "."
  },
  "compileOnSave": true
}